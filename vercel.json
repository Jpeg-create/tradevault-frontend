{
  "rewrites": [
    { "source": "/app",   "destination": "/index.html" },
    { "source": "/login", "destination": "/auth.html" },
    { "source": "/",      "destination": "/landing.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "X-Content-Type-Options", "value": "nosniff" }]
    }
  ]
}
